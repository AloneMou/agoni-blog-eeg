{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "strict": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "charset": "utf8",
    "allowJs": false,
    "pretty": true,
    "noEmitOnError": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "strictPropertyInitialization": false,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "inlineSourceMap": true,
    "importHelpers": true,
    "paths": {
      "@/*": ["./app/web/*"],
      "@@/*": ["./app/web/.umi/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "app/web/**/*",
    "typings/**/*",
    "app/web/config/**/*",
    ".prettierrc.js"
  ],
  "exclude": [
    "app/public",
    "app/views",
    "node_modules*",
    "app/web/.umi/*"
  ]
}
